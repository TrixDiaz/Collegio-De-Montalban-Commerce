{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions for admin desktop application",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:webview:default",
    "core:window:default",
    "core:app:default",
    "core:image:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "shell:allow-open",
    "shell:allow-execute",
    "fs:default",
    "fs:allow-read-text-file",
    "fs:allow-write-text-file",
    "fs:allow-read-dir",
    "fs:allow-copy-file",
    "fs:allow-create-dir",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "notification:default",
    "clipboard-manager:default",
    "clipboard-manager:allow-read-text",
    "clipboard-manager:allow-write-text"
  ]
}